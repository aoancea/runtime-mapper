<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.0.4-alpha</Version>
    <Authors>Alexandru Oancea</Authors>
    <Company>Alexandru Oancea</Company>
    <Description>A configuration free object-to-object mapper in .NET.</Description>
    <Copyright>© Alexandru Oancea. All rights reserved.</Copyright>
    <PackageLicenseUrl>https://github.com/aoancea/runtime-mapper/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/aoancea/runtime-mapper</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <RepositoryUrl>https://github.com/aoancea/runtime-mapper</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>mapper, runtime</PackageTags>
    <PackageReleaseNotes>https://github.com/aoancea/runtime-mapper/blob/master/CHANGELOG.md</PackageReleaseNotes>
    <NeutralLanguage>en-US</NeutralLanguage>
  </PropertyGroup>

</Project>
